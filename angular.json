{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "wizdm": {
      "root": "apps/wizdm/",
      "sourceRoot": "apps/wizdm/src",
      "projectType": "application",
      "prefix": "wm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/wizdm",
            "index": "apps/wizdm/src/index.html",
            "main": "apps/wizdm/src/main.ts",
            "polyfills": "apps/wizdm/src/polyfills.ts",
            "tsConfig": "apps/wizdm/tsconfig.app.json",
            "assets": [
              "apps/wizdm/src/favicon.ico",
              "apps/wizdm/src/assets"
            ],
            "styles": [
              "apps/wizdm/src/styles.scss"
            ],
            "scripts": [
              "./node_modules/hammerjs/hammer.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/wizdm/src/environments/environment.ts",
                  "with": "apps/wizdm/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wizdm:build",
            "ssl": true,
            "sslCert": "./ssl/server.crt",
            "sslKey": "./ssl/server.key"
          },
          "configurations": {
            "production": {
              "browserTarget": "wizdm:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wizdm:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/wizdm/src/test.ts",
            "polyfills": "apps/wizdm/src/polyfills.ts",
            "tsConfig": "apps/wizdm/tsconfig.spec.json",
            "karmaConfig": "apps/wizdm/karma.conf.js",
            "styles": [
              "apps/wizdm/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/wizdm/src/favicon.ico",
              "apps/wizdm/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/wizdm/tsconfig.app.json",
              "apps/wizdm/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wizdm-e2e": {
      "root": "apps/wizdm-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/wizdm-e2e/protractor.conf.js",
            "devServerTarget": "wizdm:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wizdm:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/wizdm-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "connect": {
      "root": "libs/wizdm/connect",
      "sourceRoot": "libs/wizdm/connect/src",
      "projectType": "library",
      "prefix": "wm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/wizdm/connect/tsconfig.lib.json",
            "project": "libs/wizdm/connect/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/wizdm/connect/src/test.ts",
            "tsConfig": "libs/wizdm/connect/tsconfig.spec.json",
            "karmaConfig": "libs/wizdm/connect/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/wizdm/connect/tsconfig.lib.json",
              "libs/wizdm/connect/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "content": {
      "root": "libs/wizdm/content",
      "sourceRoot": "libs/wizdm/content/src",
      "projectType": "library",
      "prefix": "wm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/wizdm/content/tsconfig.lib.json",
            "project": "libs/wizdm/content/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/wizdm/content/src/test.ts",
            "tsConfig": "libs/wizdm/content/tsconfig.spec.json",
            "karmaConfig": "libs/wizdm/content/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/wizdm/content/tsconfig.lib.json",
              "libs/wizdm/content/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "markdown": {
      "root": "libs/wizdm/markdown",
      "sourceRoot": "libs/wizdm/markdown/src",
      "projectType": "library",
      "prefix": "wm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/wizdm/markdown/tsconfig.lib.json",
            "project": "libs/wizdm/markdown/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/wizdm/markdown/src/test.ts",
            "tsConfig": "libs/wizdm/markdown/tsconfig.spec.json",
            "karmaConfig": "libs/wizdm/markdown/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/wizdm/markdown/tsconfig.lib.json",
              "libs/wizdm/markdown/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "defaultProject": "wizdm",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "styleext": "scss"
    }
  }
}