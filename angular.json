{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wizdm-app": {
      "root": "projects/wizdm-app/",
      "sourceRoot": "projects/wizdm-app/src",
      "projectType": "application",
      "prefix": "wm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/wizdm-app",
            "index": "projects/wizdm-app/src/index.html",
            "main": "projects/wizdm-app/src/main.ts",
            "polyfills": "projects/wizdm-app/src/polyfills.ts",
            "tsConfig": "projects/wizdm-app/tsconfig.app.json",
            "assets": [
              "projects/wizdm-app/src/favicon.ico",
              "projects/wizdm-app/src/assets"
            ],
            "styles": [
              "projects/wizdm-app/src/styles.scss"
            ],
            "scripts": [
              "./node_modules/hammerjs/hammer.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/wizdm-app/src/environments/environment.ts",
                  "with": "projects/wizdm-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wizdm-app:build",
            "ssl": true,
            "sslCert": "./ssl/server.crt",
            "sslKey": "./ssl/server.key"
          },
          "configurations": {
            "production": {
              "browserTarget": "wizdm-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wizdm-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wizdm-app/src/test.ts",
            "polyfills": "projects/wizdm-app/src/polyfills.ts",
            "tsConfig": "projects/wizdm-app/tsconfig.spec.json",
            "karmaConfig": "projects/wizdm-app/karma.conf.js",
            "styles": [
              "projects/wizdm-app/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/wizdm-app/src/favicon.ico",
              "projects/wizdm-app/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/wizdm-app/tsconfig.app.json",
              "projects/wizdm-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wizdm-app-e2e": {
      "root": "projects/wizdm-app-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/wizdm-app-e2e/protractor.conf.js",
            "devServerTarget": "wizdm-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wizdm-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/wizdm-app-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "connect": {
      "root": "projects/wizdm/connect",
      "sourceRoot": "projects/wizdm/connect/src",
      "projectType": "library",
      "prefix": "wm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/wizdm/connect/tsconfig.lib.json",
            "project": "projects/wizdm/connect/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wizdm/connect/src/test.ts",
            "tsConfig": "projects/wizdm/connect/tsconfig.spec.json",
            "karmaConfig": "projects/wizdm/connect/karma.conf.js"
          }
        },
        "lint": {
