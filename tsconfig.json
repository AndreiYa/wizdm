{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@wizdm/connect_prod": [
        "dist/wizdm/connect"
      ],
      "@wizdm/connect_prod/*": [
        "dist/wizdm/connect/*"
      ],
      "@wizdm/connect": [
        "projects/wizdm/connect/src/public_api"
      ],
      "@wizdm/connect/*": [
        "projects/wizdm/connect/src/lib/*"
      ],
      "@wizdm/content_prod": [
        "dist/wizdm/content"
      ],
      "@wizdm/content_prod/*": [
        "dist/wizdm/content/*"
      ],
      "@wizdm/content": [
        "projects/wizdm/content/src/public_api"
      ],
      "@wizdm/content/*": [
        "projects/wizdm/content/src/lib/*"
      ],
      "@wizdm/utils": [
        "dist/wizdm/utils"
      ],
      "@wizdm/utils/*": [
        "dist/wizdm/utils/*"
      ],
      "@wizdm/integrations": [
        "dist/wizdm/integrations"
      ],
      "@wizdm/integrations/*": [
        "dist/wizdm/integrations/*"
      ],
      "@wizdm/markdown": [
        "dist/wizdm/markdown"
      ],
      "@wizdm/markdown/*": [
        "dist/wizdm/markdown/*"
      ],
      "@wizdm/markdown_debug": [
        "projects/wizdm/markdown/src/public_api"
      ],
      "@wizdm/markdown_debug/*": [
        "projects/wizdm/markdown/src/lib/*"
      ],
      "@wizdm/elements_prod": [
        "dist/wizdm/elements"
      ],
      "@wizdm/elements_prod/*": [
        "dist/wizdm/elements/*"
      ],
      "@wizdm/elements": [
        "projects/wizdm/elements/src/public_api"
      ],
      "@wizdm/elements/*": [
        "projects/wizdm/elements/src/lib/*"
      ]
    }
  }
}